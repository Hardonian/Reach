    "@zeo/causal-skeletons": "workspace:*",
    "@zeo/constraints": "workspace:*",
    "@zeo/contracts": "workspace:*",
    "@zeo/counterfactuals": "workspace:*",
    "@zeo/dataset-builder": "workspace:*",
    "@zeo/db": "workspace:*",
    "@zeo/decision-synthesizer": "workspace:*",
    "@zeo/explain": "workspace:*",
    "@zeo/feature-discovery": "workspace:*",
    "@zeo/game": "workspace:*",
    "@zeo/hypothesis-registry": "workspace:*",
    "@zeo/id": "workspace:*",
    "@zeo/kernel": "workspace:*",
    "@zeo/lenses": "workspace:*",
