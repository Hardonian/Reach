{
  "name": "@zeo/core",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./*": null
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm -r --filter @zeo/core^... build && vitest run",
    "lint": "eslint src/**/*.ts",
    "typecheck:exports": "tsc -p tsconfig.exports.json --noEmit"
  },
  "dependencies": {
    "@zeo/analysis-planner": "workspace:*",
    "@zeo/audit": "workspace:*",
