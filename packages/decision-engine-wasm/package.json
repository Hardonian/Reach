{
  "name": "@reach/decision-engine-wasm",
  "version": "0.2.0",
  "description": "TypeScript wrapper for the decision-engine WASM module with fallback support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:wasm": "cd ../../crates/decision-engine && wasm-pack build --target nodejs --out-dir ../../packages/decision-engine-wasm/wasm",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:parity": "node test/parity.js",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist wasm"
  },
  "files": [
    "dist",
    "wasm",
    "README.md"
  ],
  "keywords": [
    "decision-engine",
    "wasm",
    "deterministic",
    "quant",
    "decision-making"
  ],
  "author": "Reach Team",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reach/decision-engine.git",
    "directory": "packages/decision-engine-wasm"
  }
}
